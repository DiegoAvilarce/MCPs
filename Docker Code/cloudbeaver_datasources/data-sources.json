{
  "folders": {},
  "connections": {
    "postgres-local": {
      "provider": "postgresql",
      "driver": "postgres-jdbc",
      "name": "PostgreSQL Local",
      "save-password": true,
      "configuration": {
        "host": "postgres",
        "port": "5432",
        "database": "${POSTGRES_DB}",
        "url": "jdbc:postgresql://postgres:5432/${POSTGRES_DB}",
        "configurationType": "MANUAL",
        "type": "dev",
        "properties": {
          "connectTimeout": "20",
          "loginTimeout": "20",
          "escapeSyntaxCallMode": "select"
        }
      },
      "credentials": {
        "user": "${POSTGRES_USER}",
        "password": "${POSTGRES_PASSWORD}"
      }
    }
  },
  "networkHandlers": {},
  "authProperties": {},
  "sharedCredentials": {}
}